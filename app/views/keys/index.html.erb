<h1>SSH Keys</h1>

<table class="table table-striped">
	<thead>
		<tr>
			<th>Name</th>
			<th>Fingerprint</th>
			<th>Created</th>
		</tr>
	</thead>	
	<tbody>
		<% @keys.each do |key| %>
		<tr>
			<td><%= key.name %></td>
			<td><%= key.fingerprint %></td>
			<td><%= key.created_at %></td>
		</tr>
		<% end %>
	</tbody>
</table>
